{
  "name": "Message Timer",
  "version": "1.2",
  "description": "Times how long it takes to send a message to a content script and back.",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["page.js"]
    }
  ],
  "browser_action": {
    "default_title": "Time to current page",
    "default_icon": "clock.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
