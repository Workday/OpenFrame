{
  "name": "A extension command test for script badges",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "commands": {
    "_execute_script_badge": {
      "suggested_key": "Ctrl+Shift+F"
    }
  },
  "permissions": [
    "tabs", "http://*/*"
  ],
  "script_badge": {
    "default_popup": "popup.html"
  }
}
