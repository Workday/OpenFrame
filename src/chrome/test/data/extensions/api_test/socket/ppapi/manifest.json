{
  "manifest_version": 2,
  "name": "chrome.socket",
  "version": "0.1",
  "description": "end-to-end browser test for chrome.socket API in Pepper",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    {
      "socket": [
        "tcp-connect",
        "tcp-listen",
        "udp-send-to",
        "udp-bind",
        "udp-multicast-membership"
      ]
    }
  ]
}
